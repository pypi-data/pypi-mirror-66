  "dependencies": {
    "@jupyterlab/application": "^1.2.4",
    "@jupyterlab/codeeditor": "^1.2.0",
    "@jupyterlab/console": "^1.2.5",
    "@jupyterlab/coreutils": "^3.2.0",
    "@jupyterlab/fileeditor": "^1.2.4",
    "@jupyterlab/notebook": "^1.2.5",
    "@jupyterlab/rendermime": "^1.2.4",
    "@jupyterlab/services": "^4.2.0",
    "@jupyterlab/tooltip": "^1.2.4",
    "@phosphor/algorithm": "^1.2.0",
    "@phosphor/coreutils": "^1.3.1",
    "@phosphor/widgets": "^1.9.0"
  },
  "devDependencies": {
    "rimraf": "~2.6.2",
    "typedoc": "^0.14.2",
    "typescript": "~3.5.1"
  },
  "publishConfig": {
    "access": "public"
